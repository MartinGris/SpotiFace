<!doctype html>
<html>
  <head>
    <title>SpotiFace</title>
		<script src="http://code.jquery.com/jquery-1.11.1.js"></script>
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
		<script src="/socket.io/socket.io.js"></script>
		<script src="javascripts/animatescroll.js"></script>
        <script src="javascripts/spotify.js"></script>
  </head>
  
  <script type="text/javascript">
    var userId = <% userId %>;
    console.log(userId);
  </script>
  
<body>

	<h1 class="text-center">SpotiFace</h1>

	<div class="row text-center"  style="margin-top: 5%;">
		<div class="col-sm-4 col-sm-offset-4">
			<input id="searchInput" class="form-control" type="text"> </input>
		</div>
	</div>

	<div class="row" style="margin-top: 5%;">
		<div id="searchResultDiv" class="col-sm-6 col-sm-offset-3 panel-collapse collapse">
			<table id="searchResult"
				class="table table-striped table-bordered table-hover table-condensed table-responsive">
				<thead>
					<tr>
						<th>Artist</th>
						<th>Title</th>
						<th class="text-center">Action</th>
					</tr>
				</thead>
				<tbody>
		
				</tbody>
			</table>
		</div>
	
	</div>

	<div class="row" style="margin-top: 5%;">
		<div id="songListDiv" class="col-sm-6 col-sm-offset-3">
			<table id="songList"
				class="table table-striped table-bordered table-hover table-condensed table-responsive">
				<thead>
					<tr>
						<th>Artist</th>
						<th>Title</th>
						<th class="text-center">Action</th>
					</tr>
				</thead>
				<tbody>
		
				</tbody>
			</table>
		</div>
	
	</div>
	<div class="row text-center">
		<iframe style="display: none;" id="iframePlay" src="" seamless="seamless" scrolling="auto" height="10px" width="10px" align="middle" scale="1.5"></iframe>
	</div>


</body>
</html>

